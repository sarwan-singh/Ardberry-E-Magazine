<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.e_bookardberrytechnologypvtltd">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.AUTHENTICATE_ACCOUNTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ebookicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ebookicon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".bookPdf"></activity>
        <activity android:name=".MostLikedBooks" />
        <activity
            android:name=".Feedback"
            android:label="Feedback" />
        <activity
            android:name=".Account"
            android:label="My Account" />
        <activity
            android:name=".PopUpLayout"
            android:theme="@style/AppTheme.PopMe" />
        <activity
            android:name=".MainActivity"
            android:label="Home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>